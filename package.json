{
  "name": "docker-browser-console",
  "version": "5.0.0",
  "description": "Forward input/output from docker containers to your browser",
  "main": "index.js",
  "browser": "browser.js",
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "dependencies": {
    "brfs": "^1.2.0",
    "computed-style": "^0.1.3",
    "defaultcss": "0.0.2",
    "docker-run": "^3.0.0",
    "duplexify": "^3.2.0",
    "ndjson": "^1.2.3",
    "term.js": "^0.0.4",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "browserify": "^5.12.1",
    "pump": "^1.0.0",
    "websocket-stream": "^1.0.2",
    "ws": "^0.4.32"
  },
  "scripts": {
    "demo": "browserify demo/index.js > demo/bundle.js; node demo/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mafintosh/docker-browser-console"
